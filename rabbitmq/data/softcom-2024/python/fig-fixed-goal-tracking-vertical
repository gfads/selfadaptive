#axes[0].plot(t, y_120, color="black",linestyle='--',lw=0.5)
axes[0].plot()
axes[0].set(xlabel='time(s)', ylabel='rate(msg/s)',title='')

axes[1].set_ylim(0, 3000)
axes[1].plot(t, y_astar, label="Worst Controller (AsTAR)", color="gray")
axes[1].plot(t, y_goal, label="Fixed Goal", color="red")
#axes[1].plot(t, y_120, color="black",linestyle='--',lw=0.5)
#axes[1].plot(t, y_80, label="Goal Range [80%-120%]", color="black",linestyle='--',lw=0.5)
axes[1].plot()
axes[1].set(xlabel='time(s)', ylabel='rate(msg/s)',title='')

# plt.title("Variable Goal")
axes[0].legend()
axes[1].legend()

fig.savefig("/content/drive/MyDrive/research/publications/events/2024-softcom/fig/fig-fixed-goal-tracking-vertical.pdf")

