#axes[1].plot(t, y_80, label="Goal Range [80%-120%]", color="black",linestyle='--',lw=0.5)
axes[1].plot()
axes[1].set(xlabel='time(s)', ylabel='rate(msg/s)',title='')

# plt.title("Variable Goal")
axes[0].legend()
axes[1].legend()

fig.savefig("/content/drive/MyDrive/research/publications/events/2024-softcom/fig/fig-variable-goal-tracking-vertical.pdf")

