import matplotlib.pyplot as plt
import numpy as np

# Create some mock data
t = np.arange(0, 500, 5)
data1 = np.arange(start=5, stop=505, step=5)
data2 = [303.908436,679.209468,950.372618,1187.591927,1323.339422,1505.262684,1621.790945,1624.864026,1979.768140,2219.855139,2398.237283,2653.302933,2356.466754,2566.299355,2504.854523,2508.593950,2607.102248,2774.443841,2632.217026,2659.166264,2754.828356,2853.581566,2944.906362,2868.891961,3058.114939,3170.227178,3265.552834,3155.742144,3341.198855,3562.311417,3454.863465,3453.747117,3427.609062,3611.610123,3727.484227,4376.346641,4230.669789,3975.027695,3891.340147,3927.356517,3926.824388,3900.046223,4061.855787,3774.672151,3923.887590,3883.888531,4107.834229,4256.355825,4314.946126,3981.089155,4146.928263,4384.085988,4324.685973,4708.115547,3936.299649,5268.334615,5060.995474,5538.434003,4524.334812,4821.976857,4861.696521,4695.798310,4794.145566,4711.973533,4862.641784,4867.100617,4838.937677,4754.834892,4993.645714,5076.559456,5343.438442,5578.381501,4755.702862,4917.447021,5978.876048,5499.714102,4929.167842,5307.061688,5188.860363,5144.124020,5493.158007,5244.402796,5259.087399,5147.858259,4901.008233,5363.275437,5310.509897,5460.178600,5722.131095,5351.769527,5239.257507,5550.858864,5974.504226,5620.234317,5589.997833,5842.922745,5830.855280,5924.837005,5993.654722,6038.680405]

print(len(data1))

fig, ax1 = plt.subplots()

color = 'black'
ax1.set_xlabel('time (s)')
ax1.set_ylabel('pc', color=color)
ax1.scatter(t, data1, color=color, marker=".")
ax1.tick_params(axis='y', labelcolor=color)

ax2 = ax1.twinx()  # instantiate a second axes that shares the same x-axis

color = 'blue'
ax2.set_ylabel('rate(msg/s)', color=color)  # we already handled the x-label with ax1
ax2.scatter(t, data2, color=color,marker=".")
ax2.tick_params(axis='y', labelcolor=color)

fig.tight_layout()  # otherwise the right y-label is slightly clipped

fig.savefig("/content/drive/MyDrive/research/publications/journals/2023-jpdc/fig-ramp-training.pdf")

plt.show()